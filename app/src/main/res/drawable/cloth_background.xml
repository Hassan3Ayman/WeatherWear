<?xml version="1.0" encoding="utf-8"?>
<shape android:shape="rectangle" xmlns:android="http://schemas.android.com/apk/res/android">
    <size android:width="300dp" android:height="300dp"/>
    <stroke
        android:width="@dimen/stroke_width"
        android:dashWidth="@dimen/spacing_small"
        android:dashGap="@dimen/gap_width"
        android:color="@color/color_dash_background" />
    <corners android:radius="12dp"/>
    <padding android:right="@dimen/spacing_small"
        android:top="@dimen/spacing_small"
        android:left="@dimen/spacing_small"
        android:bottom="@dimen/spacing_small"/>
</shape>